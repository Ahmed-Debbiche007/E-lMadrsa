<AnchorPane id="AnchorPane" prefHeight="536.0" prefWidth="1168.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AjoutQuestionController">
 <children>
